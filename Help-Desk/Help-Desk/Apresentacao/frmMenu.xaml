<Window x:Class="Help_Desk.Apresentacao.frmMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Help_Desk.Apresentacao"
        mc:Ignorable="d"
        Title="frmMenu" Height="450" Width="800">
    <Grid>
        <Menu HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="792">
            <MenuItem x:Name="mniCadastrar" Header="Cadastrar" Height="24" Width="100" Click="mniCadastrar_Click"/>
            <MenuItem x:Name="mniVisualizar" Header="Visualizar" Height="24" Width="100" Click="mniVisualizar_Click"/>
            <MenuItem x:Name="mniAdministrador" Header="Adiministrador" Height="24" Width="100"/>
        </Menu>
        <Button x:Name="btnVoltar" Content="Voltar" HorizontalAlignment="Left" Margin="659,99,0,0" VerticalAlignment="Top" Width="75" Click="btnVoltar_Click"/>

    </Grid>
</Window>
